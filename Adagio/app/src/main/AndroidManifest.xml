<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.adagio">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main_menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".music_library"
            android:label="@string/category_music_library" />
        <activity
            android:name=".on_the_rise"
            android:label="@string/category_on_the_rise" />
        <activity
            android:name=".play_screen"
            android:label="@string/category_play" />
        <activity
            android:name=".recentActivity"
            android:label="@string/recent_activity" />
        <activity android:name=".artist_order"
            android:label="Artist A-Z"/>
        <activity android:name=".Genres"
            android:label="Music Genres"></activity>
    </application>

</manifest>